/* szcv enable */
mul8 (instr, phase, szcv_enable);
input [15:0] instr;
input [2:0] phase;
output szcv_enable;

reg szcv_enable;

always @* begin
	if (phase[0]) begin
		if (instr[15:11] == 5'b10100 || instr[15:11] == 5'b10111) begin
			szcv_enable = 1'b0; // branch op koushin sinai
		end else begin
			szcv_enable = 1'b1;
		end
	end else begin
		szcv_enable = 1'b0;
	end
end

endmodule