/* Get contents of Ra and Rb from register file */

module mul1 (from_ir, from_rf, ra, rb);
	input [15:0] from_ir;
	input [15:0] from_rf;
	output [2:0] ar;
	output [2:0] br;
	reg [2:0] ar;
	reg [2:0] br;
	
	always @* begin
		ar = from_ir[13:11];
		br = from_ir[10:8];
