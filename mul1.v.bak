			end
			2'b00 :	begin
				ar = from_rf[13:11];
				br = from_rf[10:8];
			end
			2'b01 :	begin
				ar = from_rf[13:11];
				br = from_rf[10:8];
			end
			2'b10	:	begin
				ar_tmp <= 2'b00;
				br = in_rf[10:8];
			end
		endcase */
	end
endmodule